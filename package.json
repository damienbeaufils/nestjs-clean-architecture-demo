{
  "name": "nestjs-clean-architecture-demo",
  "version": "0.0.1",
  "description": "NestJS Clean Architecture demo",
  "private": true,
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "npm run start:prod",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lintfix": "npm run lint -- --fix",
    "pretest": "npm run test:cleanArchitecture && npm run lint",
    "pretest:e2e": "rimraf e2e.sqlite",
    "test": "npm run test:cov && npm run test:e2e",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config e2e/jest-e2e.json --runInBand",
    "test:cleanArchitecture": "./tools/check-clean-architecture.sh",
    "test:mutation": "stryker run",
    "pretypeorm": "ts-node -r tsconfig-paths/register database/write-ormconfig-for-migrations.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:run": "npm run typeorm -- migration:run -c schema"
  },
  "dependencies": {
    "@hapi/joi": "17.1.0",
    "@nestjs/common": "6.11.6",
    "@nestjs/core": "6.11.6",
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/typeorm": "6.2.0",
    "lodash": "4.17.21",
    "pg": "7.18.1",
    "rimraf": "3.0.2",
    "rxjs": "6.5.4",
    "sqlite3": "4.1.1",
    "typeorm": "0.2.25"
  },
  "devDependencies": {
    "@nestjs/cli": "6.14.2",
    "@nestjs/schematics": "6.9.3",
    "@nestjs/testing": "6.11.6",
    "@stryker-mutator/core": "2.5.0",
    "@stryker-mutator/html-reporter": "2.5.0",
    "@stryker-mutator/jest-runner": "2.5.0",
    "@stryker-mutator/typescript": "2.5.0",
    "@types/express": "4.17.2",
    "@types/hapi__joi": "16.0.9",
    "@types/jest": "25.1.2",
    "@types/lodash": "4.14.149",
    "@types/node": "12.12.26",
    "@types/supertest": "2.0.8",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "supertest": "4.0.2",
    "ts-jest": "25.2.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "tsc-watch": "4.1.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.1.0",
    "typescript": "3.7.5"
  },
  "engines": {
    "node": "~12.15"
  }
}
